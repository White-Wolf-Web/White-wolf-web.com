{
    "redirects": [
      {
        "source": "/(.*)",
        "destination": "https://www.white-wolf-web.com/$1",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Strict-Transport-Security",
            "value": "max-age=31536000; includeSubDomains; preload"
          },
          {
            "key": "X-Frame-Options",
            "value": "SAMEORIGIN"
          }
        ]
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "continue": true
      }
    ]
  }
  