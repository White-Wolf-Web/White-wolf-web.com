{
    "redirects": [
      {
        "source": "/robots.txt",
        "destination": "/robots.txt",
        "permanent": false
      },
      {
        "source": "/sitemap.xml",
        "destination": "/sitemap.xml",
        "permanent": false
      },
      {
        "source": "/(.*)",
        "destination": "https://www.white-wolf-web.com/$1",
        "permanent": true,
        "statusCode": 301
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Strict-Transport-Security",
            "value": "max-age=31536000; includeSubDomains; preload"
          },
          {
            "key": "X-Frame-Options",
            "value": "SAMEORIGIN"
          }
        ]
      }
    ]
  }
  