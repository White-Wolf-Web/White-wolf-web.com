{
    "routes": [
      {
        "src": "/(.*)",
        "headers": {
          "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload",
          "X-Frame-Options": "SAMEORIGIN"
        },
        "continue": true
      },
      {
        "src": "/(.*)",
        "dest": "/index.html",
        "continue": true
      },
      {
        "src": "/(.*)",
        "dest": "https://www.white-wolf-web.com/$1",
        "status": 301
      }
    ]
  }
  